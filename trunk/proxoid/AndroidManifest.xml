<?xml version="1.0" encoding="utf-8"?>
<manifest 
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.proxoid"
	android:versionCode="9" 
	android:versionName="1.0.9"
>
      
    <uses-sdk 
    	android:minSdkVersion="1"
    />
    <supports-screens 
    	android:smallScreens="true" 
		android:normalScreens="true" 
		android:largeScreens="true" 
		android:anyDensity="true"
    />
     
    <uses-permission android:name="android.permission.INTERNET" />
      
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true" android:installLocation="auto">
        <activity android:name=".Proxoid" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".ProxoidService"/>
        
        <!-- The application's publisher ID assigned by AdMob -->
		<meta-data android:value="a14a4d226a81c03" android:name="ADMOB_PUBLISHER_ID"/>
        
    </application>
    
</manifest> 
